package phones;

public class SamsungS4 extends Phone {
 
public SamsungS4() {
	screenSize = 5;
	hasWifi = true;
	}
@Override
public void sendSMS(String number, String message) {
	System.out.println("Hello" + number + message);
}
public static void main(String[] args) {
	// TODO Auto-generated method stub
	SamsungS4 s1 = new SamsungS4();
	System.out.println(s1.getScreenSize());
	System.out.println(s1.isHasWifi());
	s1.sendSMS("0959215201", "Have a nice day!");
}
}
